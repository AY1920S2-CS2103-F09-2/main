<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Circle?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" fx:id="calendarGridDay" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11">
    <children>
        <Circle fx:id="circleDay" fill="#ffffff00" radius="20.0" stroke="#ffffff00" strokeType="INSIDE" />
        <Label fx:id="calendarDay" alignment="CENTER" contentDisplay="CENTER" text="\$day" textAlignment="CENTER" />
    </children>
</StackPane>
