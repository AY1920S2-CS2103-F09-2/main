<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<StackPane fx:id="calendarGridDay" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Circle fx:id="circleDay" fill="#ffffff00" radius="20.0" stroke="#ffffff00" strokeType="INSIDE" />
      <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
         <children>
              <Label fx:id="calendarDay" alignment="CENTER" contentDisplay="CENTER" text="\$day" textAlignment="CENTER" />
            <Circle fx:id="dotTasks" fill="#ffffff00" radius="4.0" stroke="#ffffff00" strokeType="INSIDE" />
         </children>
      </VBox>
    </children>
</StackPane>
